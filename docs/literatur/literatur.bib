@mastersthesis{Master_Array_Mueller,
  abstract      = {In dieser Arbeit wird eine Methode entwickelt, mit der man die Positionen zweier Sprecher im Raum lokalisieren kann. F{\"u}r die Lokalisierung werden zwei {\"a}quidistante lineare Mikrofonarrays mit je vier Mikrofonen verwendet. F{\"u}r die Richtungserkennung dient der Multi-Channel Cross Correlation Algorith- mus. Danach werden die Positionen der Sprecher mittels Kreuzpeilung be- rechnet. Die entwickelte Methode wird in MATLAB simuliert, auf dem TI DSP D.Module.C6713 implementiert und in Echtzeitumgebung getestet.},
  author        = {Markus M{\"u}ller},
  date-added    = {2013-05-07 04:30:18 +0000},
  date-modified = {2013-05-07 04:34:37 +0000},
  keywords      = {MCCC, Mikrofonarray, Sprecherlokalisierung},
  school        = {HAW-Hamburg},
  title         = {Sprecherlokalisierung von zwei oder mehr Sprechern im Raum unter Verwendung von Mikrofonarrays},
  type          = {Master Thesis},
  year          = {2012}
}

@mastersthesis{Master_Array_Pikora,
  abstract      = {In dieser Arbeit werden die Algorithmen Multichannel Crosscorrelation und Spatial
                   Linear Prediction f{\"u}r eine Sprecherloaklisierung auf einem Signalprozessor untersucht.
                   Die Lokalisierung erfolgt mit einem achtkanaligen Mikrofonarray, wahlweise in
                   linearem oder zirkularem Aufbau. Desweiteren ist eine variable Verz{\"o}gerungskette
                   implementiert, mit der die Signale unverz{\"o}gert zueinander aus dem Signalprozessor
                   ausgegeben werden. Beide Algorithmen wurden in MATLAB simuliert und in Echtzeitumgebung
                   getestet. Die Implementierung auf dem D.Module.C6713 erfolgt in der
                   Programmiersprache C.},
  author        = {Kolja Pikora},
  date-added    = {2013-05-07 04:27:50 +0000},
  date-modified = {2013-05-07 04:30:13 +0000},
  school        = {HAW-Hamburg},
  title         = {Korrelationsalgorithmen zur Sprecherlokalisierung mittels DSP-basiertem Echtzeitsystem und Mikrofonarray},
  type          = {Master Thesis},
  year          = {2012}
}

